<section class="blogs">
    {{#each blogs as |blog|}}
    <div class="card">
        <div class="card-header">
            {{> blog.title}}
        </div>
        <div class="card-body">
            <blockquote class="blockquote mb-0">
            <p>{{blog.details}}</p>
        </div>
    </div>
    {{> blog-details}}
    {{/each}}
</section>